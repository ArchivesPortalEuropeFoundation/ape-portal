<div class="title">
    [[!+tiptitle:eq=`0`:then=`
        [[!+label]]
    `:else=`
    <div class="tipTitle">
        <div class="tipIcon" data-tooltip-content="#searchFilter[[!+tip_id]]Tooltip">
            <i class="far fa-question-circle"></i>
        </div>
        [[!+label]]
    </div>
    `]]
</div>
<div class="inner" data-facet-set="[[!+identifier]]">

    <div class="searchLight">
        <div class="inputWrapper">
            <i class="fas fa-search"></i>
            <input data-g="search-filter" data-search-target="[[!+identifier]]" type="text" name="search" placeholder="[[!+find_label]]" autocomplete="off"  />
        </div>
    </div>


    <div class="checkboxList">
        
    </div>
</div>
