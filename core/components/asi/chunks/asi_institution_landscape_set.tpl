<div class="countryList">
    <h5><i class="fas fa-globe-europe"></i> [[!+country_name]]</h5>
    [[!+items]]
    <div class="row moreListInst">
        [[!+more_items]]
        <div class="col-xs-12">
            <div class="text-center mt30">
                <a class="showMore">[[!%asi.action_show_more_from? &topic=`actions` &namespace=`asi`]] [[!+country_name]] <i class="far fa-angle-down"></i></a>
                <a class="showLess">[[!%asi.action_show_less_from? &topic=`actions` &namespace=`asi`]] [[!+country_name]] <i class="far fa-angle-up"></i></a>
            </div>
        </div>
    </div>
</div>