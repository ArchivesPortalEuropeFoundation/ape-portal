<li class="checkbox" data-facet-item-set="[[!+filter_field]]" data-facet-item="[[!+filter_value]]" data-search-filter-item="[[!+set_identifier]]">
    <input [[!+exclusive:eq=`1`:then=`data-filter-exclusive="[[!+exclusive]]"`]]  data-filter-field="[[!+filter_field]]" data-filter-value="[[!+filter_value]]" data-filter-type="[[!+filter_type]]" type="checkbox" name="[[!+name]]" [[!+selected:is=`1`:then=`checked='checked'`]]>
    <span>[[!+name]]</span> <span data-info="result_count">[[!+result_count]]</span>
</li>